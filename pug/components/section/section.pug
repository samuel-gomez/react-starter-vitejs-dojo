mixin Section({ name, background= '', transition = "fade", iframe, interactive=false, autoAnimate= false  })
    - const backgroundIframe = iframe ? { 'data-background-iframe':iframe } : {}
    - const backgroundInteractive = interactive ? { 'data-background-interactive':interactive } : {}
    - const dataAutoAnimate = autoAnimate ? { 'data-auto-animate':autoAnimate } : {}
    - const t = { 'data-background':background, 'data-background-transition':transition, ...backgroundIframe, ...backgroundInteractive, ...dataAutoAnimate }
    section(class=`${prefix}-${name}`, id=name, )&attributes({...t})
        block